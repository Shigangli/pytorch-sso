{
  "dataset": "CIFAR-10",
  "epochs": 50,
  "batch_size": 1024,
  "val_batch_size": 5000,
  "random_crop": false,
  "random_horizontal_flip": false,
  "normalizing_data": true,
  "arch_file": "models/lenet.py",
  "arch_name": "LeNet5BatchNorm",
  "optim_name": "SecondOrderOptimizer",
  "optim_args": {
    "curv_type":"Cov",
    "curv_shapes": {
      "Conv2d": "Kron",
      "Linear": "Kron",
      "BatchNorm1d": "Diag",
      "BatchNorm2d": "Diag"
    },
    "lr": 1e-3,
    "momentum": 0.9,
    "momentum_type": "raw",
    "weight_decay": 1e-3,
    "acc_steps": 1
  },
  "curv_args": {
    "damping": 1e-3,
    "ema_decay": 0.95,
    "pi_type": "tracenorm"
  }
}